{% trans_default_domain 'FOSUserBundle' %}

<div class="fos_user_user_show">
    <p>{{ 'profile.show.username'|trans }}: {{ user.username }}</p>
    <p>{{ 'profile.show.email'|trans }}: {{ user.email }}</p>
    <p>{{ 'profile.show.nom' |trans }}: {{ user.getNomMembre() }}</p>
    <p>{{ 'profile.show.prenom' |trans }}: {{ user.getPrenomMembre() }}</p>
    <p>{{ 'profile.show.adresse' |trans }}: {{ user.getAdresseMembre() }}</p>
    <p>{{ 'profile.show.datenais' |trans }}: {{ user.getDateNaisMembre() | date('d/m/y')}}</p>
    <p>{{ 'profile.show.numtel' |trans }}: {{ user.getNumtelMembre() }}</p>
    <a  href="{{ path('fos_user_profile_edit') }}" class="btn btn-primary btn-sm">Edit Profile</a>

</div>
